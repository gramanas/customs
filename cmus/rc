set output_plugin=alsa
set dsp.alsa.device=default
set mixer.alsa.device=default
set mixer.alsa.channel=Master

set format_treewin= %l (%y)
set format_trackwin_album= %l [%y]
set format_trackwin=  %2n %t %{?y?}%= %d 
set format_current= %a // %l // %t%=%y 
set format_playlist= %a :: %l :: %t%=%2n %y %5d 

set time_show_leading_zero=false

bind -f common q shell tmux detach-client -s cmus

colorscheme gruvbox